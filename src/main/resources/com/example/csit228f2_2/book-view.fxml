<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.csit228f2_2.InsertBook">
    <children>
        <VBox alignment="CENTER" prefHeight="386.0" prefWidth="600.0" spacing="10">
            <Label style="-fx-font-size: 20px;" text="Add New Book" />
            <HBox alignment="CENTER" spacing="10">
                <Label text="Title:" />
                <TextField fx:id="titleField" />
            </HBox>
            <HBox alignment="CENTER" spacing="10">
                <Label text="Genre:" />
                <TextField fx:id="genreField" />
            </HBox>
            <HBox alignment="BOTTOM_CENTER" prefHeight="48.0" prefWidth="600.0">
                <Button onAction="#addBook" text="Add Book" />
            </HBox>
            <HBox alignment="BOTTOM_CENTER" prefHeight="48.0" prefWidth="600.0">
                <children>
                    <Button fx:id="back4" mnemonicParsing="false" onAction="#goBackClick4" text="See Dashboard">
                    </Button>
                </children>
            </HBox>
        </VBox>
    </children>
</AnchorPane>
