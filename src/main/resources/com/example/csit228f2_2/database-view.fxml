<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="anchor2" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.csit228f2_2.RegisterPage">
   <children>
      <VBox layoutX="-2.0" layoutY="18.0" prefHeight="368.0" prefWidth="600.0">
         <children>
             <HBox alignment="CENTER" prefHeight="38.0" prefWidth="600.0">
                 <children>
                     <Label text="REGISTER">
                     <font>
                        <Font size="22.0" />
                     </font></Label>
                 </children>
             </HBox>
            <HBox alignment="CENTER" prefHeight="59.0" prefWidth="600.0">
               <children>
                  <Label text="First Name : " />
                  <TextField fx:id="fname" />
               </children>
            </HBox>
             <HBox alignment="CENTER" prefHeight="59.0" prefWidth="600.0">
                 <children>
                     <Label text="Last Name : " />
                     <TextField fx:id="lname" />
                 </children>
             </HBox>
             <HBox alignment="CENTER" prefHeight="59.0" prefWidth="600.0">
                 <children>
                     <Label text="Username : " />
                     <TextField fx:id="uname" />
                 </children>
             </HBox>
             <HBox alignment="CENTER" prefHeight="59.0" prefWidth="600.0">
                 <children>
                     <Label text="Password: " />
                     <PasswordField fx:id="pword" />
                 </children>
             </HBox>
             <HBox alignment="CENTER" prefHeight="59.0" prefWidth="600.0">
                 <children>
                     <Label text="Email : " />
                     <TextField fx:id="emailField" />
                 </children>
             </HBox>
            <HBox alignment="CENTER" prefHeight="48.0" prefWidth="600.0">
               <children>
                  <Button fx:id="submitButton" mnemonicParsing="false" text="Submit" />
               </children>
            </HBox>
             <HBox alignment="BOTTOM_CENTER" prefHeight="48.0" prefWidth="600.0">
                 <children>
                     <Button fx:id="back1" mnemonicParsing="false" onAction="#goBackClick" text="Go Back">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin></Button>
                 </children>
             </HBox>
         </children>
      </VBox>
   </children>

</AnchorPane>
